{
  "javascript.validate.enable": false,
  "editor.formatOnSave": true,
  "flow.useNPMPackagedFlow": true,
  "eslint.options": {
    "rules": {
      "no-debugger": "off"
    }
  },
  "json.schemas": [
    {
      "fileMatch": ["cypress.json"],
      "url": "https://on.cypress.io/cypress.schema.json"
    }
  ],
  "path-intellisense.mappings": {
    "/": "${workspaceRoot}"
  },
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    ".vscode",
    "typings",
    "**/docker/cypress/fixtures/**"
  ],
  "cSpell.words": [
    "Builtins",
    "Cascader",
    "Libring",
    "Plotly",
    "admob",
    "adroll",
    "aggr",
    "appannie",
    "appletv",
    "appstore",
    "arial",
    "autoload",
    "captcha",
    "chainable",
    "checkboxes",
    "checkmark",
    "cobertura",
    "codemods",
    "collapsable",
    "comparator",
    "corejs",
    "correlator",
    "creatable",
    "crosshair",
    "csrf",
    "csrftoken",
    "datapoints",
    "datasets",
    "datasource",
    "datasources",
    "datetime",
